{
    "version": 2,
      "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.4" }
    },
    "routes": [
        {
            "src": "/dist/(.*)",
            "dest": "/public/dist/"
        },
        {
            "src": "/storage/(.*)",
            "dest": "/public/storage/"
        },
        {
            "src": "/contoh/(.*)",
            "dest": "/public/dist/contoh/"
        },
        {
            "src": "/css/(.*)",
            "dest": "/public/dist/css/"
        },
        {
            "src": "/img/(.*)",
            "dest": "/public/dist/img/"
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/dist/js/"
        },
        {
            "src": "/libs/(.*)",
            "dest": "/public/dist/libs/"
        },
        {
            "src": "/static/(.*)",
            "dest": "/public/dist/static/"
        },
        {
            "src": "/gambar/(.*)",
            "dest": "/public/storage/gambar"
        },
        {
            "src": "/tmp/(.*)",
            "dest": "/public/storage/tmp"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "local",
        "APP_DEBUG": "true",
        "APP_URL": "",
 
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
 
        "CACHE_DRIVER": "file",
        "LOG_CHANNEL": "stack",
        "SESSION_DRIVER": "file"
    }
}
